{
  "expo": {
    "name": "TootDesk",
    "slug": "tootdesk",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "tootdesk",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-center.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/d7f07c8c-ca06-4dd3-a754-d09eb5287ad5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1",
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "top.thedesk.toot",
      "infoPlist": {
        "CFBundleDevelopmentRegion": "ja_JP"
      }
    },
    "locales": {
      "en": "./i18n/bundle/en.json",
      "ja": "./i18n/bundle/ja.json"
    },
    "android": {
      "versionCode": 1,
      "package": "top.thedesk.toot"
    },
    "extra": {
      "eas": {
        "projectId": "d7f07c8c-ca06-4dd3-a754-d09eb5287ad5"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
